{
    "repositories": [
        {
            "name": "phpactor",
            "url": "git@github.com:phpactor/phpactor",
            "vars": {
                "composerLock": true,
                "defaultBranch": "develop",
                "branches": [
                    "master",
                    "develop"
                ],
                "jobs": [
                    "phpactor-vim",
                    "phpunit",
                    "phpactor-deps-and-exts",
                    "phpactor-docs"
                ]
            },
            "tags": ["phpactor"]
        },
        {
            "name": "amp-fswatch",
            "url": "git@github.com:phpactor/amp-fswatch",
            "tags": ["lib"]
        },
        {
            "name": "class-mover",
            "url": "git@github.com:phpactor/class-mover",
            "tags": ["lib"]
        },
        {
            "name": "class-to-file",
            "url": "git@github.com:phpactor/class-to-file",
            "tags": ["lib"]
        },
        {
            "name": "class-to-file-extension",
            "url": "git@github.com:phpactor/class-to-file-extension",
            "tags": ["ext"]
        },
        {
            "name": "code-builder",
            "url": "git@github.com:phpactor/code-builder",
            "tags": ["lib"]
        },
        {
            "name": "code-transform",
            "url": "git@github.com:phpactor/code-transform",
            "tags": ["lib"]
        },
        {
            "name": "code-transform-extension",
            "url": "git@github.com:phpactor/code-transform-extension",
            "tags": ["ext"]
        },
        {
            "name": "completion",
            "url": "git@github.com:phpactor/completion",
            "vars": {
                "checkoutOptions": {
                    "fetch-depth": 0
                }
            },
            "tags": ["lib"]
        },
        {
            "name": "completion-extension",
            "url": "git@github.com:phpactor/completion-extension",
            "tags": ["ext"]
        },
        {
            "name": "completion-rpc-extension",
            "url": "git@github.com:phpactor/completion-rpc-extension",
            "tags": ["ext"]
        },
        {
            "name": "completion-worse-extension",
            "url": "git@github.com:phpactor/completion-worse-extension",
            "tags": ["ext"]
        },
        {
            "name": "composer-autoloader-extension",
            "url": "git@github.com:phpactor/composer-autoloader-extension",
            "tags": ["ext"]
        },
        {
            "name": "config-loader",
            "url": "git@github.com:phpactor/config-loader",
            "vars": {
                "phpMin": "^7.3 || ^8.0"
            },
            "tags": ["lib"]
        },
        {
            "name": "console-extension",
            "url": "git@github.com:phpactor/console-extension",
            "tags": ["ext"]
        },
        {
            "name": "container",
            "url": "git@github.com:phpactor/container",
            "vars": {
                "phpMin": "^7.3 || ^8.0"
            },
            "tags": ["lib"]
        },
        {
            "name": "debug-extension",
            "url": "git@github.com:phpactor/debug-extension",
            "tags": ["ext"]
        },
        {
            "name": "docblock",
            "url": "git@github.com:phpactor/docblock",
            "tags": ["lib"]
        },
        {
            "name": "extension-manager-extension",
            "url": "git@github.com:phpactor/extension-manager-extension",
            "tags": ["ext"]
        },
        {
            "name": "file-path-resolver",
            "url": "git@github.com:phpactor/file-path-resolver",
            "tags": ["lib"]
        },
        {
            "name": "file-path-resolver-extension",
            "url": "git@github.com:phpactor/file-path-resolver-extension",
            "tags": ["ext"]
        },
        {
            "name": "indexer-extension",
            "url": "git@github.com:phpactor/indexer-extension",
            "tags": ["ext"]
        },
        {
            "name": "language-server",
            "url": "git@github.com:phpactor/language-server",
            "tags": ["lib"]
        },
        {
            "name": "language-server-extension",
            "url": "git@github.com:phpactor/language-server-extension",
            "tags": ["ext"]
        },
        {
            "name": "language-server-phpactor-extensions",
            "url": "git@github.com:phpactor/language-server-phpactor-extensions",
            "tags": ["ext"]
        },
        {
            "name": "language-server-protocol",
            "url": "git@github.com:phpactor/language-server-protocol",
            "vars": {
                "jobs": [
                    "phpstan",
                    "phpunit"
                ]
            },
            "tags": ["lib"]
        },
        {
            "name": "logging-extension",
            "url": "git@github.com:phpactor/logging-extension",
            "tags": ["ext"]
        },
        {
            "name": "map-resolver",
            "url": "git@github.com:phpactor/map-resolver",
            "vars": {
                "phpMin": "^7.3 || ^8.0"
            },
            "tags": ["lib"]
        },
        {
            "name": "name",
            "url": "git@github.com:phpactor/name",
            "tags": ["lib"]
        },
        {
            "name": "path-finder",
            "url": "git@github.com:phpactor/path-finder",
            "tags": ["lib"]
        },
        {
            "name": "php-extension",
            "url": "git@github.com:phpactor/php-extension",
            "tags": ["ext"]
        },
        {
            "name": "reference-finder",
            "url": "git@github.com:phpactor/reference-finder",
            "tags": ["lib"]
        },
        {
            "name": "reference-finder-extension",
            "url": "git@github.com:phpactor/reference-finder-extension",
            "tags": ["ext"]
        },
        {
            "name": "reference-finder-rpc-extension",
            "url": "git@github.com:phpactor/reference-finder-rpc-extension",
            "tags": ["ext"]
        },
        {
            "name": "rpc-extension",
            "url": "git@github.com:phpactor/rpc-extension",
            "tags": ["ext"]
        },
        {
            "name": "source-code-filesystem",
            "url": "git@github.com:phpactor/source-code-filesystem",
            "tags": ["lib"]
        },
        {
            "name": "source-code-filesystem-extension",
            "url": "git@github.com:phpactor/source-code-filesystem-extension",
            "tags": ["ext"]
        },
        {
            "name": "test-utils",
            "url": "git@github.com:phpactor/test-utils",
            "tags": ["lib"]
        },
        {
            "name": "text-document",
            "url": "git@github.com:phpactor/text-document",
            "tags": ["lib"]
        },
        {
            "name": "worse-reference-finder-extension",
            "url": "git@github.com:phpactor/worse-reference-finder-extension",
            "tags": ["ext"]
        },
        {
            "name": "worse-reference-finder",
            "url": "git@github.com:phpactor/worse-reference-finder",
            "tags": ["lib"]
        },
        {
            "name": "worse-reflection",
            "url": "git@github.com:phpactor/worse-reflection",
            "tags": ["lib"]
        },
        {
            "name": "worse-reflection-extension",
            "url": "git@github.com:phpactor/worse-reflection-extension",
            "tags": ["ext"]
        }
    ]
